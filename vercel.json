{
  "installCommand": "npm install --force",
  "buildCommand": "rm -rf node_modules/.vite node_modules/.cache dist .vite && npm run build",
  "rewrites": [
    {
      "source": "/api/piped",
      "destination": "/api/piped"
    },
    {
      "source": "/api/piped-streams",
      "destination": "/api/piped-streams"
    },
    {
      "source": "/((?!api/).*)",
      "destination": "/index.html"
    }
  ]
}
